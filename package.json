{
  "name": "zaraz-app",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "upload": "wrangler pages deploy",
    "logs": "wrangler pages deployment tail --project-name zaraz-app",
    "dev": "remix vite:dev",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "wrangler pages dev ./build/client",
    "typecheck": "tsc",
    "typegen": "wrangler types",
    "db.local": "wrangler d1 execute d1-zaraz-app --local --file=./migrations/schema-001.sql",
    "db.remote": "wrangler d1 execute d1-zaraz-app --remote --file=./migrations/schema-001.sql"
  },
  "dependencies": {
    "@remix-pwa/sw": "^3.0.5",
