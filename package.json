    "@remix-run/react": "^2.9.2",
    "arktype": "1.0.29-alpha",
    "clsx": "^2.1.1",
    "googleapis": "^137.1.0",
    "isbot": "^5.1.6",
    "jwt-decode": "^4.0.0",
    "miniflare": "^3.20240419.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240502.0",
