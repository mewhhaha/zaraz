{
  "name": "zaraz-app",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "VITE_VERSION=$(git rev-parse HEAD) && remix vite:build",
    "upload": "wrangler pages deploy",
    "logs": "wrangler pages deployment tail --project-name zaraz-app",
    "dev": "remix vite:dev",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "wrangler pages dev ./build/client",
    "typecheck": "tsc",
    "typegen": "wrangler types",
    "db.local": "wrangler d1 execute d1-zaraz-app --local --file=./migrations/schema-001.sql",
    "db.remote": "wrangler d1 execute d1-zaraz-app --remote --file=./migrations/schema-001.sql"
  },
  "dependencies": {
    "@remix-run/cloudflare": "^2.12.1",
    "@remix-run/cloudflare-pages": "^2.12.1",
    "@remix-run/react": "^2.12.1",
    "arktype": "2.0.0-rc.13",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "googleapis": "^144.0.0",
    "isbot": "^5.1.17",
    "jwt-decode": "^4.0.0",
    "miniflare": "^3.20240925.0",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240925.0",
    "@eslint/js": "^9.11.1",
    "@mewhhaha/little-fetcher": "^0.1.5",
    "@remix-pwa/dev": "^3.1.0",
    "@remix-run/dev": "^2.12.1",
    "@remix-run/node": "^2.12.1",
    "@types/eslint": "^9.6.1",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.2.7",
    "autoprefixer": "^10.4.20",
    "esbuild": "^0.24.0",
    "eslint": "^9.11.1",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-hooks": "^0.4.3",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-react": "^7.37.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-tailwindcss": "^3.17.4",
    "globals": "^15.9.0",
    "node-fetch": "^3.3.2",
    "postcss": "^8.4.47",
    "react-confetti-explosion": "^2.1.2",
    "tailwind-merge": "^2.5.2",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.7.0",
    "vite": "^5.4.8",
    "vite-tsconfig-paths": "^5.0.1",
    "wrangler": "^3.78.12"
  },
  "engines": {
    "node": ">=20.0.0"
  },
