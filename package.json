{
  "name": "zaraz-app",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "VITE_VERSION=$(git rev-parse HEAD) && remix vite:build",
    "upload": "wrangler pages deploy",
    "logs": "wrangler pages deployment tail --project-name zaraz-app",
    "dev": "remix vite:dev",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "wrangler pages dev ./build/client",
    "typecheck": "tsc",
    "typegen": "wrangler types",
    "db.local": "wrangler d1 execute d1-zaraz-app --local --file=./migrations/schema-001.sql",
    "db.remote": "wrangler d1 execute d1-zaraz-app --remote --file=./migrations/schema-001.sql"
  },
  "dependencies": {
    "@remix-run/cloudflare": "^2.10.2",
    "@remix-run/cloudflare-pages": "^2.10.2",
    "@remix-run/react": "^2.10.2",
    "arktype": "2.0.0-dev.29",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "googleapis": "^140.0.1",
    "isbot": "^5.1.12",
    "jwt-decode": "^4.0.0",
    "miniflare": "^3.20240701.0",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240620.0",
    "@eslint/js": "^9.6.0",
    "@mewhhaha/little-fetcher": "^0.1.5",
    "@remix-pwa/dev": "^3.0.5",
    "@remix-run/dev": "^2.10.2",
    "@remix-run/node": "^2.10.2",
    "@types/eslint": "^8.56.10",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.2.7",
    "autoprefixer": "^10.4.19",
    "esbuild": "^0.23.0",
    "eslint": "^8.57.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-hooks": "^0.4.3",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-tailwindcss": "^3.17.4",
    "globals": "^15.8.0",
    "node-fetch": "^3.3.2",
    "postcss": "^8.4.39",
    "react-confetti-explosion": "^2.1.2",
    "tailwind-merge": "^2.4.0",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.16.0",
    "vite": "^5.3.3",
    "vite-tsconfig-paths": "^4.2.1",
    "wrangler": "^3.63.2"
  },
  "engines": {
    "node": ">=20.0.0"
  },
